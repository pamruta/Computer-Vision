{
	"filename": "NY Driving License",
	"global_date_format": "mm-dd-yy",
	"regexes":
	[
		{
			"match_regex": "ID:\\s*\\d{3} \\d{3} \\d{3}",
			"strip_expr": "ID:\\s*",
			"field_name": ["LICENSE NO"]
		},
		{
			"match_regex": "DOB:\\s*\\d\\d-\\d\\d-\\d\\d",
			"data_type": "DATE",
			"field_name": ["DOB"],
			"strip_expr": "DOB:\\s*"
		},
		{
			"match_regex": "ISSUED:\\s*\\d\\d-\\d\\d-\\d\\d",
			"data_type": "DATE",
			"field_name": ["ISSUE DATE"],
			"strip_expr": "ISSUED:\\s*"
		},
		{
			"match_regex": "EXPIRES:\\s*\\d\\d-\\d\\d-\\d\\d",
			"data_type": "DATE",
			"field_name": ["EXPIRY DATE"],
			"strip_expr": "EXPIRES:\\s*"
		},
		{
			"match_regex": "SEX:\\s*[A-Z]",
			"strip_expr": "SEX:\\s*",
			"field_name": ["SEX"]
		},
		{
			"match_regex": "EYES:\\s*[A-Z]{2}",
			"strip_expr": "EYES:\\s*",
			"field_name": ["EYE COLOR"]
		},
		{
			"match_regex": "HT:\\s*\\d-\\d\\d",
			"strip_expr": "HT:\\s*",
			"field_name": ["HEIGHT"]
		}
	]
}
